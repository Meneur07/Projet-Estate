      <Title>propBase</Title>
    </Header>
    <Snippet>
      <Code Language="CSharp">
        <![CDATA[
        public $type$ $name$
        {
            get { return GetProperty<$type$>(); }
            set { SetProperty<$type$>(value); }
          
        }
        ]]>
      </Code>
    </Snippet>
  </CodeSnippet>
